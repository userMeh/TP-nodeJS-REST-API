    "dependencies": {
        "express": "^4.18.3",
        "mysql2": "^3.9.2",
        "ts-node": "^10.9.2",
        "tsx": "^4.7.1",
        "typeorm": "^0.3.20",
        "zod": "^3.22.4"
    },
    "devDependencies": {
        "@types/express": "^4.17.21",
        "prettier": "3.2.5"
    }
}
